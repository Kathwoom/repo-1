import java.util.Stack;

public class stack1 {
    public static void main(String[] args) {
        Stack<Integer> istk = new Stack<>();
        istk.push(10);
        istk.push(20);
        istk.push(30);

        System.out.println("Peek at the top item: " + istk.peek()); 
        System.out.println("Position of 20 in the stack: " + istk.search(20)); 
        System.out.println("Position of 15 in the stack: " + istk.search(15)); 


        System.out.println("Popped item: " + istk.pop());
        System.out.println("Popped item: " + istk.pop());

        Stack<String> stk = new Stack<>();
        stk.push("Hello");
        stk.push("World");

        System.out.println("Popped item: " + stk.pop());
        System.out.println("Popped item: " + stk.pop());

        System.out.println("Is intStack empty? " + istk.isEmpty());
        System.out.println("Is stringStack empty? " + stk.isEmpty());
    }
}
