#include<stdio.h>
#include<conio.h>
void main()
{
 int a[10],i,item,n;
 printf("Linear Search\n-----------------\n");
 printf("\nEnter the array limit:");
 scanf("%d",&n);
 printf("\nEnter the array:");
 for(i=0;i<n;i++)
  scanf("%d",&a[i]);
 printf("\nEnter element to find:");
 scanf("%d",&item);
 for(i=0;i<n;i++)
 { 
  if(a[i]==item)
  {
   printf("\nitem found");
   break;
   }
 }
 if(i==n)
  printf("\nItem not found");
 getch();
 }
  
   
